<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="529.0" prefWidth="811.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="s.schedulingsystemvia.controllers.LessonListViewController">
<children>
    <TableView fx:id="lessonListTable" layoutX="14.0" layoutY="58.0" prefHeight="320.0" prefWidth="780.0">
        <columns>
            <TableColumn fx:id="courseColumn" prefWidth="150.0" text="Course" />
            <TableColumn fx:id="classroomColumn" prefWidth="150.0" text="Classroom" />
            <TableColumn fx:id="startTimeColumn" prefWidth="150.0" text="Start Time" />
            <TableColumn fx:id="endTimeColumn" prefWidth="150.0" text="End Time" />
        </columns>
    </TableView>
    <Label fx:id="errorLabel" layoutX="404.0" layoutY="414.0" text="errorLabel" textFill="RED" />
    <TextField fx:id="search" layoutX="480.0" layoutY="21.0" prefHeight="25.0" prefWidth="98.0" promptText="Search course" />
    <Button layoutX="586.0" layoutY="21.0" mnemonicParsing="false" onAction="#searchButton" text="Search" />
    <Button layoutX="29.0" layoutY="410.0" mnemonicParsing="false" onAction="#addCourseButton" text="Add course" />
    <Button layoutX="185.0" layoutY="410.0" mnemonicParsing="false" onAction="#removeCourseButton" text="Remove " />
    <Button layoutX="326.0" layoutY="410.0" mnemonicParsing="false" onAction="#onCustomizeCourse" text="Edit" />
    <TextField fx:id="programmeTextField" layoutX="20.0" layoutY="21.0" prefHeight="25.0" prefWidth="85.0" promptText="Programme" />
    <TextField fx:id="semesterTextField" layoutX="115.0" layoutY="21.0" prefHeight="25.0" prefWidth="70.0" promptText="Semester" />
    <TextField fx:id="classTextField" layoutX="195.0" layoutY="21.0" prefHeight="25.0" prefWidth="52.0" promptText="Class" />
    <DatePicker fx:id="startDateProperty" layoutX="249.0" layoutY="21.0" prefHeight="25.0" prefWidth="109.0" promptText="Start Date" />
    <DatePicker fx:id="endDateProperty" layoutX="359.0" layoutY="21.0" prefHeight="25.0" prefWidth="91.0" promptText="End Date" />
</children>
</AnchorPane>
